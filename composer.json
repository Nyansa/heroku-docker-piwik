{
    "name": "dz0ny/heroku-docker-piwik",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "piwik",
                "version": "3.5.1",
                "type": "project",
                "dist": {
                    "type": "zip",
                    "url": "https://builds.piwik.org/piwik-3.5.1.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "matomo/plugin-CustomDimensions",
                "version": "3.1.3",
                "type": "piwik-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://plugins.matomo.org/api/2.0/plugins/CustomDimensions/download/3.1.3"
                },
                "extra": {
                  "installer-name": "CustomDimensions"
                }
            }
        }
    ],
    "authors": [
        {
            "name": "Janez Troha",
            "email": "dz0ny@ubuntu.si"
        }
    ],
    "require": {
        "php": "~7.1",
        "piwik": "*",
        "matomo/plugin-CustomDimensions": "*",
        "ext-mbstring": "*",
        "ext-gd": "*"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "*"
    }
}
